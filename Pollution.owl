    <!-- http://ontology.eil.utoronto.ca/GCI/Environment/Pollution.owl#Air_pollution -->

    <owl:Class rdf:about="&Pollution;Air_pollution">
        <rdfs:subClassOf rdf:resource="&Pollution;Pollution"/>
        <rdfs:comment>The different types of air pollution that are represented in the ISO37120 Environment Indicators.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/GCI/Environment/Pollution.owl#Air_pollution_concentration -->

    <owl:Class rdf:about="&Pollution;Air_pollution_concentration">
        <rdfs:subClassOf rdf:resource="&Pollution;Pollution_concentration"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&Pollution;measures"/>
                <owl:onClass rdf:resource="&Pollution;Air_pollution"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&om-1;unit_of_measure"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/GCI/Foundation/GCI-Foundation.owl#microgram_per_cubic_metre"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Measurements of air pollution at a specific time and location.</rdfs:comment>
    </owl:Class>
    


